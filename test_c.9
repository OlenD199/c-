#include <stdio.h>
#include <math.h>

int fun(int a)
{
	int b = 0;
	switch (a)//switch(c)  语句中，c可以是char类型，因为char 以ascii码的形式存在
	{
	case 1:b = 10;
	case 2:b = 20;
	case 3:b = 30;
	default:b = 0;
	}
	return b;
}

int main()
{
	int a=fun(1);
	printf("%d", a);
	return 0;
}



//找素数优化算法
int main()
{
	int i = 0;
	int j = 0;
	int count = 0;
	for (i = 101; i < 200; i+=2)
	{
		int ret = sqrt(i);
		for (j = 2; j <= ret; j++)
		{
			if (0 == i % j)
			{
				break;
			}
		}
		if (j > ret)
		{
			printf("%d ", i);
			count++;
		}
	}
	printf("\n%d\n", count);

	return 0;
}

//编程数一下1 - 100 有多少个包含9的整数
int main()
{
	int i = 0;
	int count = 0;
	for (i = 0; i < 100; i++)
	{
		if (i % 10 == 9)//计算个位上有多少个9
		{
			count++;
		}
		if(i / 10 == 9)//计算十位上有多少个9
		{
			count++;
		}
	}
	printf("%d\n", count);

	return 0;
}

//计算1/1 - 1/2 + 1/3 - 1/4 + 1/5 ..... +1/99 - 1/100
int main()
{
	int i = 0;
	int temp = 1;
	float sum = 0;
	for (i = 1; i < 101; i++)
	{
		sum = sum + (1.0 / i) * temp;
		temp = temp * (-1);
	}
	printf("%f", sum);
	return 0;
}


//在屏幕上输出9*9乘法表
int main()
{
	int i;
	int j;
	for (i = 1; i < 10; i++)
	{
		for (j = 1; j < i + 1; j++)
		{
			printf("%d*%d=%-2d   ", j, i, j * i);   //%2d叫做右对齐，不够两位前面的输出空格，
													//%-2d叫左对齐，不够两位的后面输出空格
		}
		printf("\n");
	}

	return 0;
}
